	<!-- Adds medicinal honey item -->
	<append xpath="/items">
		<item name="foodMedicalHoney">
			<property name="HoldType" value="3"/>
			<property name="DisplayType" value="foodCureInfection"/>
			<property name="CustomIcon" value="drinkJarGrainAlcohol"/> <property name="CustomIconTint" value="e3d54f"/>
